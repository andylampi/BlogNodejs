doctype html
html(lang="en")
    block head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        style 
            include ./css/index.css
        title blog Node
    block body 
    nav
        a(id="homeNav") Home 
        a(id="postNav") Post
        a(id="profileNav", href=routeStatus)= status
    #post
        #card1
            h3= Post[0].title
        #card2 
            h3= Post[1].title
        #card3
            h3= Post[2].title
        #card4
            h3= Post[3].title
        #card5
            h3= Post[4].title
        #card6
            h3= Post[5].title



